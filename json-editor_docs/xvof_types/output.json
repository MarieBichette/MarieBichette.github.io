{
  "type": "object",
  "options": {
    "disable_collapse": true,
    "disable_properties": true
  },
  "properties": {
    "number-of-images":{
      "type": "integer",
      "options": {
        "infoText": "Nombre d'images dans la simulation"
      }
    },
    "database": {
      "$ref": "o-database.json"
    },
    "variables": {
      "type": "array",
      "format": "table",
      "uniqueItems": true,
      "items": {
        "type": "string",
        "enum": [
          "All",
          "ArtificialViscosity",
          "CellSize",
          "CohesiveForce",
          "Density",
          "DeviatoricStress",
          "DiscontinuityOpening",
          "EquivalentPlasticStrainRate",
          "InternalEnergy",
          "NodeVelocity",
          "NodeCoordinates",
          "PlasticStrainRate",
          "Porosity",
          "Pressure",
          "ShearModulus",
          "SoundVelocity",
          "Stress",
          "YieldStress"
        ]
      },
      "default": ["All"],
      "options": {
        "infoText": "Sélection des variables à sortir",
        "collapsed": true
      }
    }
  },
  "additionalProperties":false,
  "required": ["number-of-images", "database", "variables"]
}
